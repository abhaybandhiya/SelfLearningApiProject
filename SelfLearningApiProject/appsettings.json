{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //"DefaultConnection": "Server=COMPUTER000193;Database=SelfLearningDbDatabase;Trusted_Connection=True;TrustServerCertificate=True;"
    "DefaultConnection": "Server=COMPUTER000255;Database=SelfLearningDbDatabase;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "Key": "super-secret-key-please-change-to-env-or-vault-1234567890",
    "Issuer": "AbhayAPP",
    "Audience": "AbhayClients",
    "DurationMinutes": "60"
  },
  "FileSettings": {
    "UploadPath": "Uploads"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/System Event log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }

}
